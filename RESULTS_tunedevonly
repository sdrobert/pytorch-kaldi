#! /usr/bin/env bash
for x in exp/*/decode_dev*; do
	if [ -d $x ]; then
		# find the best WER for the dev set
		dev_wer=$(
			echo $x |
			grep "${1:-.*}" >/dev/null &&
			grep Sum $x/score_*/*.sys 2> /dev/null |
			utils/best_wer.sh)
		echo ${dev_wer}
		test_file=$(echo "${dev_wer}" | awk '{print $(NF)}' | sed 's/decode_dev/decode_test/g')
		grep -H Sum ${test_file} 2> /dev/null | utils/best_wer.sh
	fi
done
exit 0

# Experiment results
# to run stats on these values, try
# $ cat RESULTS_tunedevonly | sed 's/# //g' | python stats.py descriptive
# $ cat RESULTS_tunedevonly | sed 's/# //g' | grep LSTM | grep -v '_si' | grep 'test' | python stats.py friedman
# $ cat RESULTS_tunedevonly | sed 's/# //g' | grep LSTM | grep 'fbank_' | grep 'test' | python stats.py wilcoxon
# $ cat RESULTS_tunedevonly | sed 's/# //g' | grep liGRU | grep -v '_si' | grep 'test' | python stats.py friedman
# $ cat RESULTS_tunedevonly | sed 's/# //g' | grep liGRU | grep 'fbank' | grep 'test' | python stats.py wilcoxon
# $ feats=( kaldi fmllr fbank gbank tonebank ); for i in $(seq 0 $(expr ${#feats[@]} - 2)); do feat_a=${feats[i]}; for j in $(seq $(expr $i + 1) $(expr ${#feats[@]} - 1)); do feat_b=${feats[j]}; paste <(echo "${feat_a}-${feat_b}") <(cat RESULTS_tunedevonly | sed 's/# //g' | grep test | grep liGRU | grep -v '_si' | grep "${feat_a}\|${feat_b}" | python stats.py wilcoxon --independent-variable 'feature'); done; done
